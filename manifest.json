{
  "author": "creedowl",
  "name": "selector",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "selector"
  },
  "permissions": [ "activeTab" ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
